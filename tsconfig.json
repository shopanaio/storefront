{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@src/*": ["./src/*"],
      "@codegen/*": ["./codegen/*"],
      "@checkout/*": ["./src/modules/checkout/*"],
      "@shopana/entity": ["./packages/entity/src/index.ts"],
      "shopana-utils": ["./packages/shopana-utils/dist/index.js"],
      "shopana-utils/*": ["./packages/shopana-utils/dist/*"],
      "@shopana/brand": ["./packages/brand-default/src"],
      "@shopana/brand/*": ["./packages/brand-default/src/*"],
      "@stackflow/react/future": [
        "./src/modules/storefront-box-builder-module/stack-engine/index.tsx"
      ],
      "@stackflow/react": [
        "./src/modules/storefront-box-builder-module/stack-engine/index.tsx"
      ],
      "@stackflow/config": [
        "./src/modules/storefront-box-builder-module/stack-engine/index.tsx"
      ],
      "@stackflow/plugin-basic-ui": [
        "./src/modules/storefront-box-builder-module/stack-engine/index.tsx"
      ],
      "@stackflow/plugin-basic-ui/*": [
        "./src/modules/storefront-box-builder-module/stack-engine/*"
      ],
      "@stackflow/plugin-history-sync": [
        "./src/modules/storefront-box-builder-module/stack-engine/index.tsx"
      ],
      "@stackflow/plugin-renderer-basic": [
        "./src/modules/storefront-box-builder-module/stack-engine/index.tsx"
      ]
    },
    "target": "esnext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "types": ["webpack-env", "node", "next"],
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": ["next-env.d.ts", ".next/types/**/*.ts", "**/*.ts", "**/*.tsx", "**/*.d.ts"],
  "exclude": ["node_modules"]
}
