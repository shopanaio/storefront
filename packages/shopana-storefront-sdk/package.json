{
  "name": "@shopana/storefront-sdk",
  "version": "0.1.0",
  "private": false,
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src/next"
  ],
  "exports": {
    "./next/page": "./src/next/page.tsx",
    "./package.json": "./package.json",
    ".": "./dist/index.js",
    "./*": "./dist/*"
  },
  "scripts": {
    "relay": "CMS=shopana ../../node_modules/.bin/relay-compiler relay.shopana.json",
    "build": "yarn relay && tsc -p tsconfig.json && node scripts/copy-generated.cjs",
    "dev": "tsc -p tsconfig.json --watch",
    "clean": "rimraf dist",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "peerDependencies": {
    "js-cookie": "^3.0.0",
    "next": "^14.0.0 || ^15.0.0 || ^16.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "react-relay": "^18.0.0 || ^19.0.0 || ^20.0.0",
    "relay-runtime": "^18.0.0 || ^19.0.0 || ^20.0.0",
    "zustand": "^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.14.1",
    "@types/react": "19.2.2",
    "@types/react-dom": "^18.0.0 || ^19.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "jsdom": "^26.0.0",
    "next": "16.0.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "vitest": "^3.0.5"
  },
  "dependencies": {
    "path-to-regexp": "^8.3.0"
  }
}
