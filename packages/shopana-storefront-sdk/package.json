{
  "name": "@shopana/storefront-sdk",
  "version": "0.1.0",
  "private": false,
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src/next"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "default": "./dist/core/index.js"
    },
    "./shop": {
      "types": "./dist/shop/index.d.ts",
      "default": "./dist/shop/index.js"
    },
    "./sdk": {
      "types": "./dist/sdk/index.d.ts",
      "default": "./dist/sdk/index.js"
    },
    "./graphql": {
      "types": "./dist/graphql/index.d.ts",
      "default": "./dist/graphql/index.js"
    },
    "./graphql/relay": {
      "types": "./dist/graphql/relay/index.d.ts",
      "default": "./dist/graphql/relay/index.js"
    },
    "./cms": {
      "types": "./dist/cms/index.d.ts",
      "default": "./dist/cms/index.js"
    },
    "./next": {
      "types": "./dist/next/index.d.ts",
      "default": "./dist/next/index.js"
    },
    "./next/relay": {
      "types": "./dist/next/relay/index.d.ts",
      "default": "./dist/next/relay/index.js"
    },
    "./next/page": {
      "types": "./src/next/page.tsx",
      "default": "./src/next/page.tsx"
    },
    "./model": {
      "types": "./dist/model/index.d.ts",
      "default": "./dist/model/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "clean": "rimraf dist",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "peerDependencies": {
    "next": "^14.0.0 || ^15.0.0 || ^16.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "react-relay": "^18.0.0 || ^19.0.0 || ^20.0.0",
    "relay-runtime": "^18.0.0 || ^19.0.0 || ^20.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.14.1",
    "@types/react": "19.2.2",
    "@types/react-dom": "^18.0.0 || ^19.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "jsdom": "^26.0.0",
    "next": "16.0.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "vitest": "^3.0.5"
  },
  "dependencies": {
    "path-to-regexp": "^8.3.0"
  }
}
